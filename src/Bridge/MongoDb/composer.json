{
    "name": "symfony/ai-mongo-db-store",
    "description": "MongoDB Atlas vector store bridge for Symfony AI",
    "license": "MIT",
    "type": "symfony-ai-store",
    "keywords": [
        "ai",
        "atlas",
        "bridge",
        "mongodb",
        "store",
        "vector"
    ],
    "authors": [
        {
            "name": "Christopher Hertel",
            "email": "mail@christopher-hertel.de"
        },
        {
            "name": "Oskar Stark",
            "email": "oskarstark@googlemail.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "require": {
        "php": ">=8.2",
        "mongodb/mongodb": "^1.21|^2.0",
        "psr/log": "^3.0",
        "symfony/ai-platform": "@dev",
        "symfony/ai-store": "@dev",
        "symfony/uid": "^7.3|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.13"
    },
    "conflict": {
        "mongodb/mongodb": "<1.21"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Symfony\\AI\\Store\\Bridge\\MongoDb\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\AI\\Store\\Bridge\\MongoDb\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        },
        "thanks": {
            "name": "symfony/ai",
            "url": "https://github.com/symfony/ai"
        }
    }
}
